spApi.sessionStorageSet('psControl','1');}},webPushUtilities:{makeRequest:function(url,data,successCallback){var request={type:'POST',url:url,data:data};if(successCallback){request.success=successCallback;}
sQuery.ajax(request);},isUserCompatibleWithCampaign:function(lang){return spApi.isOnProductPage()&&spApi.getLang()===lang;},baseSensorParameters:function(campaignId,product){return{partnerName:'tikivn',campaignId:campaignId,productId:product.id,url:product.url,imageUrl:product.img}}},priceDrop:function(webPush){this.webPush=webPush;this.utilities=spApi.webPushUtilities;this.setPriceDropData=function(){this.priceDropData=JSON.parse(this.priceDropLocalStorageData)||{stats:{},eligibilitySentProducts:{}};if(!this.priceDropData.eligibilitySentProducts[this.currency]){this.priceDropData.eligibilitySentProducts[this.currency]=[];}
