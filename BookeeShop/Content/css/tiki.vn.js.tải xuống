if(window.admicro_cpa_tiki=window.admicro_cpa_tiki||{domain:"tiki.vn"},window.admicro_cpa_tiki.getCookie=function(e){return document.cookie.length>0&&(c_start=document.cookie.indexOf(e+"="),-1!=c_start)?(c_start=c_start+e.length+1,c_end=document.cookie.indexOf(";",c_start),-1==c_end&&(c_end=document.cookie.length),unescape(document.cookie.substring(c_start,c_end))):""},window.admicro_cpa_tiki.setCookie=function(e,i,o,t){var c=new Date;c.setDate(c.getDate()+o),document.cookie=e+"="+escape(i)+(null==o?"":";path=/;domain="+t+";expires="+c.toUTCString())},"/checkout/success"==document.location.pathname){var a=document.getElementsByClassName("well well-sm"),num=1,money=0;if(a.length>0){value="OrderComplete-"+num+"-"+money;var ordId=a[0].innerHTML.replace(/[^0-9]+/g,"");value=value+"###"+ordId,window.admicro_cpa_tiki.setCookie("__v",value,30,window.admicro_cpa_tiki.domain),window.admicro_cpa_tiki.setCookie("__su",1,30,window.admicro_cpa_tiki.domain)}}